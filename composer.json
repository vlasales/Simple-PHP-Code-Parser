{
    "name": "simple-php-code-parser",
    "description": "",
    "type": "library",
    "keywords": [
        "php"
    ],
    "homepage": "https://github.com/vlasales/Simple-PHP-Code-Parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Moelleken",
            "homepage": "http://www.moelleken.org/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "phpdocumentor/reflection-docblock": "~4.3|~5.1",
        "nikic/php-parser": "~4.4",
        "vimeo/psalm": "~3.11"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0 || ~7.0"
    },
    "autoload": {
        "psr-4": {
            "SimplePhpParser\\": "src/"
        },
        "files": [
            "src/Model/types/ArrayArray.php",
            "src/Model/types/ArrayFloat.php",
            "src/Model/types/ArrayInt.php",
            "src/Model/types/ArrayString.php",
            "src/Model/types/BooleanFalse.php",
            "src/Model/types/BooleanTrue.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
